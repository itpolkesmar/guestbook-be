// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/config/client"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guest {
  id         String      @id
  name       String
  instansi   String
  ingroup    String
  visitors   Int
  purpose    String
  to_meet    String
  scheduled  String
  created_at DateTime
  updated_at DateTime
}

model User {
  id String @id
  nip Int
  password String
  name String
  gedung String
  created_at DateTime
  updated_at DateTime
}
